{
  "name": "IngatSolat for Chrome",
  "version": "1.0",
  "description": "Reminding Muslims in Singapore to pray!",
  "permissions": [ "activeTab", "declarativeContent", "storage" ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "options_page": "configure/configure.html",
  "page_action": {
    "default_popup": "toolbar_popup/toolbar_popup.html",
    "default_icon": {
      "16": "assets/images/ingatsolat_16.png",
      "32": "assets/images/ingatsolat_32.png",
      "48": "assets/images/ingatsolat_48.png",
      "128": "assets/images/ingatsolat_128.png"
    }
  },
  "icons": {
    "16": "assets/images/ingatsolat_16.png",
    "32": "assets/images/ingatsolat_32.png",
    "48": "assets/images/ingatsolat_48.png",
    "128": "assets/images/ingatsolat_128.png"
  },
  "manifest_version": 2
}